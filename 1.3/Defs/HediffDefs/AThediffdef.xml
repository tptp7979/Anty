	<HediffDef ParentName="AddictionBase">
		<defName>AntyQueenneed</defName>
		<label>Nutritional jelly need</label>
		<description>nutritious jelly must be consumed constantly. If don't, body will be slowly destroyed.</description>
		<isBad>false</isBad>
		<hediffClass>Hediff_Addiction</hediffClass>
		<causesNeed>AntyEggbag_need</causesNeed>
		<scenarioCanAdd>false</scenarioCanAdd>
		<everCurableByItem>false</everCurableByItem>
		<initialSeverity>0.2</initialSeverity>
		<stages>
		<li>
			<becomeVisible>false</becomeVisible>
		</li>
		<li>
			<becomeVisible>false</becomeVisible>
			<label>Bad nutrition</label>
			<hediffGivers>
				<li Class="HediffGiver_Random">
					<hediff>Badnutritiondamage</hediff>
					<mtbDays>0.5</mtbDays>
					<partsToAffect>
						<li>Nose</li>
						<li>Finger</li>
						<li>Toe</li>
						<li>Eye</li>
						<li>Ear</li>
						<li>Arm</li>
						<li>Leg</li>
						<li>Head</li>
					</partsToAffect>
				</li>
			</hediffGivers>
		</li>
		</stages>
	</HediffDef>
	
	<HediffDef>
		<defName>AntyQueenbirth</defName>
		<label>AntyQueen</label>
		<description>This entity has egg sacs. It requires the constant intake of nutrition jelly, and will regularly lay larvae.</description>
		<isBad>false</isBad>
		<hediffClass>HediffWithComps</hediffClass>
		<scenarioCanAdd>false</scenarioCanAdd>
		<initialSeverity>1</initialSeverity>
		<maxSeverity>1.0</maxSeverity>
		<everCurableByItem>false</everCurableByItem>
		<comps>
			<li Class="HediffCompProperties_Discoverable">
				<sendLetterWhenDiscovered>true</sendLetterWhenDiscovered>
				<discoverLetterLabel>Queen birth</discoverLetterLabel>
				<discoverLetterText>{0}'s body has grown egg sacs. It will regularly lay larvae, given a steady supply of nutritious jelly. Otherwise, the body will slowly destroy itself.</discoverLetterText>
				<letterType>AT_Queenbirth</letterType>
				<messageType>PositiveEvent</messageType>
			</li>
		</comps>
		<stages>
		<li>
			<minSeverity>0</minSeverity>
			<hediffGivers>
				<li Class="HediffGiver_Random">
					<hediff>Pregnant_B</hediff>
					<mtbDays>1</mtbDays>
				</li>
				<li Class="AntyHediffGiver_Random">
					<hediff>AntyQueenneed</hediff>
					<mtbDays>0.001</mtbDays>
				</li>
				<li Class="AntyHediffGiver_Random">
					<hediff>Badnutritiondamage</hediff>
					<mtbDays>0.001</mtbDays>
					<partsToAffect>
						<li>AntyTool</li>
					</partsToAffect>
				</li>
			</hediffGivers>
			<statFactors>
				<IncomingDamageFactor>1.5</IncomingDamageFactor>
			</statFactors>
		</li>
		</stages>
	</HediffDef>
	
