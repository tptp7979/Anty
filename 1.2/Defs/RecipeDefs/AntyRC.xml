<Defs>

	<RecipeDef>
		<defName>AntyCorpseFlesh</defName>
		<label>Corpses assembly(Heart)</label>
		<description>Using corpses to create an Organic matter management core.</description>
		<descriptionHyperlinks><ThingDef>Anty_Recyclecore</ThingDef></descriptionHyperlinks>
		<workerCounterClass>RecipeWorkerCounter_ButcherAnimals</workerCounterClass>
		<jobString>Corpses assembly(Heart)</jobString>
		<workAmount>1150</workAmount>
		<workSpeedStat>ButcheryFleshSpeed</workSpeedStat>
		<workSkill>Crafting</workSkill>
		<efficiencyStat>ButcheryFleshEfficiency</efficiencyStat>
		<effectWorking>ButcherFlesh</effectWorking>
		<soundWorking>Recipe_ButcherCorpseFlesh</soundWorking>
		<interruptIfIngredientIsRotting>true</interruptIfIngredientIsRotting>
		<uiIconThing>Anty_Recyclecore</uiIconThing>
		<ingredients>
			<li>
				<filter>
				  <categories>
					<li>Corpses</li>
				  </categories>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
					<li>Heart</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<products>
			<Anty_Recyclecore>1</Anty_Recyclecore>
		</products>
		<fixedIngredientFilter>
			<categories>
				<li>Corpses</li>
			</categories>
			<thingDefs>
				<li>Heart</li>
			</thingDefs>
			<disallowedCategories>
				<li>CorpsesMechanoid</li>
				<li>CorpsesAnimal</li>
			</disallowedCategories>
			<specialFiltersToDisallow>
				<li>AllowRotten</li>
			</specialFiltersToDisallow>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<categories>
				<li>CorpsesHumanlike</li>
			</categories>
			<thingDefs>
				<li>Heart</li>
			</thingDefs>
		</defaultIngredientFilter>
		<skillRequirements>
			<Medicine>3</Medicine>
		</skillRequirements>
		<researchPrerequisite>Mediumcolony</researchPrerequisite>
	</RecipeDef>
	
