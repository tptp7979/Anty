<Defs>

 <!-- 리플리움 가스 -->
 
	<ThingDef ParentName="ApparelBase">
		<defName>AT_Apparel_CombatSmoke</defName>
		<label>Refleum Gaspack</label>
		<description>An equipment developed by Anty to suppress enemies' sights and movements. It sprays acidified refleum gas over a large area of the battlefield.</description>
		<techLevel>Spacer</techLevel>
		<comps>
			<li Class="CompProperties_Reloadable">
				<maxCharges>1</maxCharges>
				<ammoDef>Refleum</ammoDef>
				<ammoCountToRefill>10</ammoCountToRefill>
				<baseReloadTicks>60</baseReloadTicks>
				<soundReload>Standard_Reload</soundReload>
				<hotKey>Misc4</hotKey>
				<chargeNoun>Refleum gas launcher</chargeNoun>
				<displayGizmoWhileUndrafted>false</displayGizmoWhileUndrafted>
			</li>
		</comps>
			<verbs>
				<li>
					<verbClass>Verb_LaunchProjectileStatic</verbClass>
					<label>pop Refleum Gaspack</label>
					<hasStandardCommand>true</hasStandardCommand>
					<defaultProjectile>BulletCombatSmokeLauncher</defaultProjectile>
					<warmupTime>0.5</warmupTime>
					<range>29.9</range>
					<minRange>7.1</minRange>
					<forcedMissRadius>1.9</forcedMissRadius>
					<burstShotCount>1</burstShotCount>
					<soundCast>Shot_IncendiaryLauncher</soundCast>
					<muzzleFlashScale>14</muzzleFlashScale>
					<targetParams>
						<canTargetLocations>true</canTargetLocations>
					</targetParams>
					<consumeFuelPerShot>1</consumeFuelPerShot>
					<targetable>true</targetable>
					<nonInterruptingSelfCast>true</nonInterruptingSelfCast>
					<onlyManualCast>true</onlyManualCast>
				</li>
			</verbs>
		<recipeMaker>
			<unfinishedThingDef>UnfinishedPack</unfinishedThingDef>
			<researchPrerequisite>hydroheliumion</researchPrerequisite>
			<recipeUsers>
				<li>AntyCopyTableMachining</li>
			</recipeUsers>
			<useIngredientsForColor>false</useIngredientsForColor>
			<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
			<workSkill>Crafting</workSkill>
			<effectWorking>Smith</effectWorking>
			<soundWorking>Recipe_Smith</soundWorking>
			<skillRequirements>
				<Crafting>10</Crafting>
			</skillRequirements>
		</recipeMaker>
		<costList>
			<AntyExoskeleton>20</AntyExoskeleton>
			<Refleum>10</Refleum>
			<ComponentSpacer>1</ComponentSpacer>
		</costList>
		<graphicData>
			<texPath>Things/Weapons/AT_CombatSmoke</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>TransparentPostLight</shaderType>
		</graphicData>
		<tickerType>Normal</tickerType>
		<statBases>
			<WorkToMake>3200</WorkToMake>
			<Mass>3</Mass>
			<Flammability>0.4</Flammability>
			<EquipDelay>2</EquipDelay>
		</statBases>
		<thingCategories>
				<li>ATApparel</li>
		</thingCategories>
			<generateAllowChance>0</generateAllowChance>
		<apparel>
			<careIfWornByCorpse>false</careIfWornByCorpse>
			<careIfDamaged>false</careIfDamaged>
			<wearPerDay>0</wearPerDay>
			<bodyPartGroups>
				<li>Waist</li>
			</bodyPartGroups>
			<layers>
				<li>Belt</li>
			</layers>
		</apparel>
	</ThingDef>
	
	<ThingDef ParentName="BaseBullet">
		<defName>BulletCombatSmokeLauncher</defName>
		<label>Refleum gas launcher</label>
		<graphicData>
			<texPath>Things/Weapons/lan</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>TransparentPostLight</shaderType>
			<color>(0.6, 0.2, 0.2)</color>
		</graphicData>
		<thingClass>Projectile_Explosive</thingClass>
		<projectile>
			<speed>50</speed>
			<damageDef>AT_AcidBomb_B</damageDef>
			<damageAmountBase>1</damageAmountBase>
			<preExplosionSpawnThingDef>Filth_ApproachSmoke_B</preExplosionSpawnThingDef>
			<preExplosionSpawnChance>1</preExplosionSpawnChance>
			<explosionRadius>6.9</explosionRadius>
			<explosionDelay>30</explosionDelay>
		</projectile>
	</ThingDef>

